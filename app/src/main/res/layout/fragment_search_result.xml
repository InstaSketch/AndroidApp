<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    tools:context="io.github.instasketch.instasketch.fragments.SearchResultFragment">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/local_frag_text"/>

    <include
        android:id="@+id/searchResults"
        android:clickable="true"
        layout="@layout/material_recyclerview_search"/>

    <include
        android:id="@+id/slidingQueryImage"
        layout="@layout/slidinglayer_query_image"/>

</FrameLayout>
